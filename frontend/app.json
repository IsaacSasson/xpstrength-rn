{
  "expo": {
    "name": "XPStrength",
    "slug": "xpstrength",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "XPStrength",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "package": "com.elicky.XPStrength",
      "supportsTablet": true
    },
    "android": {
      "package": "com.elicky.XPStrength",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "assetBundlePatterns": ["assets/**"],

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
